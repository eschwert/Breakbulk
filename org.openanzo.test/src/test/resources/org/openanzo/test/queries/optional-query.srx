    xmlns:xs="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://www.w3.org/2005/sparql-results#" >
  <head>
    <variable name="g"/>
    <variable name="s"/>
    <variable name="o1"/>
    <variable name="o2"/>
    <variable name="o"/>
    <variable name="o5"/>
  </head>
  <results>
    <result>
      <binding name="g"><uri>http://example.org/optional-query</uri></binding>
      <binding name="s"><uri>http://example.org/s1</uri></binding>
      <binding name="o"><literal>o4$</literal></binding>
      <binding name="o1"><uri>http://example.org/o1</uri></binding>
      <binding name="o2"><uri>http://example.org/o2</uri></binding>
    </result>
  </results>
</sparql>
