@prefix xsd       : <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .

ex:timepart {
    ex:date1 ex:p "2004-03-01"^^xsd:date .
    ex:date2 ex:p "2004-03-01Z"^^xsd:date .
    ex:date3 ex:p "2004-03-01+03:00"^^xsd:date .
    ex:date4 ex:p "2004-03-01-08:00"^^xsd:date .

    ex:dt1 ex:p "2006-11-02T12:00:01+01:00"^^xsd:dateTime .
    ex:dt2 ex:p "2006-11-02T12:00:01Z"^^xsd:dateTime .
    ex:dt3 ex:p "2006-11-02T12:00:01+08:00"^^xsd:dateTime .

    ex:t1 ex:p "13:00:00+01:00"^^xsd:time .
    ex:t2 ex:p "11:00:00.55+01:00"^^xsd:time .
    ex:t3 ex:p "11:00:00Z"^^xsd:time .
    ex:t4 ex:p "11:00:00-10:00"^^xsd:time .
}
