<propertyDefinitions>
	<propertiesGroup packageName="org.openanzo.datasource" groupPrefix="org.openanzo.datasource" className="Datasource">
		<description>Base configuration properties that are used by datasources.</description>
		<property name="isPrimary" key="KEY_IS_PRIMARY" passDefault="false" type="Boolean">
			<description>Is this the primary datasource</description>
		</property>
		<property name="initFiles" key="KEY_INIT_FILES" passDefault="false" type="String">
			<description>Init files for datasource initialization and reset</description>
		</property>
		<property name="datasourceURI" key="KEY_DATASOURCE_URI" passDefault="false" type="String">
			<description>URI for datasource</description>
		</property>
		<property name="uriPatterns" key="KEY_URI_PATTERNS" passDefault="false" type="String">
			<description>URI patterns for datasource</description>
		</property>
		<property name="enableCaching" key="KEY_ENABLE_CACHING" passDefault="true" type="Boolean">
			<description>Enable Caching</description>
		</property>
        <property name="resetEnabled" key="KEY_RESET_ENABLED" passDefault="false" type="Boolean">
            <description>Enable Reset</description>
        </property>
        <property name="maxWriteConnections" key="KEY_MAX_WRITE_CONNECTIONS" passDefault="false" type="Integer" min="0" max="1600">
            <description>Maximum number of write connections.</description>
            <example></example>
        </property>
        <property name="maxQueryConnections" key="KEY_MAX_QUERY_CONNECTIONS" passDefault="false" type="Integer" min="0" max="1600">
            <description>Maximum number of query connections.</description>
            <example></example>
        </property>
    </propertiesGroup>
</propertyDefinitions>