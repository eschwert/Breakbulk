service.pid=org.openanzo.security.ldap.LdapAuthentication
org.openanzo.services.enabled=true
org.openanzo.security.ldap.roleBaseDN=ou=groups,dc=openanzo,dc=org
org.openanzo.security.ldap.rolesSearch=(&(member={0})(objectclass=groupOfNames))
org.openanzo.security.ldap.userBaseDN=ou=users,dc=openanzo,dc=org
org.openanzo.security.ldap.sysadminRole=cn=sysadmins
org.openanzo.security.ldap.userSearch=(uid={0})
org.openanzo.security.ldap.userIdAttribute=
org.openanzo.security.ldap.useEmbeddedServer=true
org.openanzo.security.ldap.roleObjectClass=groupOfNames
org.openanzo.security.ldap.userObjectClass=person
org.openanzo.security.ldap.anonymousAccessEnabled=true
