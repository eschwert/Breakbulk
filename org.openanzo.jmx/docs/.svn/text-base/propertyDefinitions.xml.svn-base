<propertyDefinitions>
    <propertiesGroup packageName="org.openanzo.jmx" groupPrefix="org.openanzo.jmx" className="JMX">
        <description>Base configuration properties that are used by services if there aren't service specific values present.</description>
        <property name="host" key="KEY_HOST" passDefault="true" type="String">
            <description>Host name for server</description>
             <example>localhost or 127.0.0.1</example>
        </property>
        <property name="port" key="KEY_PORT" passDefault="true" type="Integer" min="1" max="65536">
            <description>Port for server</description>
             <example>8080</example>
        </property>
    </propertiesGroup>
</propertyDefinitions>