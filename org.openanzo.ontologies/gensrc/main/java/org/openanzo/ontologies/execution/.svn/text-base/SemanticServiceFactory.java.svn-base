	/**
	 * Return a list of compatible interfaces for the given type.  Searches through all ontology classes
	 * in the SemanticService ontology.
	 * @param type the type for which to find compatible interfaces
	 * @return a List of type java.lang.Class
	 */
	public static java.util.List<java.lang.Class<? extends org.openanzo.rdf.jastor.Thing>> listCompatibleInterfaces (org.openanzo.rdf.Resource type) {
		java.util.List<java.lang.Class<? extends org.openanzo.rdf.jastor.Thing>> types = new java.util.ArrayList<java.lang.Class<? extends org.openanzo.rdf.jastor.Thing>>();
		if (type.equals(org.openanzo.ontologies.execution.JavaSemanticService.TYPE)) {
			types.add(org.openanzo.ontologies.execution.JavaSemanticService.class);
		}
		if (type.equals(org.openanzo.ontologies.execution.JavascriptSemanticService.TYPE)) {
			types.add(org.openanzo.ontologies.execution.JavascriptSemanticService.class);
		}
		if (type.equals(org.openanzo.ontologies.execution.SemanticOperation.TYPE)) {
			types.add(org.openanzo.ontologies.execution.SemanticOperation.class);
		}
		if (type.equals(org.openanzo.ontologies.execution.SemanticService.TYPE)) {
			types.add(org.openanzo.ontologies.execution.SemanticService.class);
		}
		if (type.equals(org.openanzo.ontologies.execution.StateStyleEnum.TYPE)) {
			types.add(org.openanzo.ontologies.execution.StateStyleEnum.class);
		}
		return types;
	}
