dojo.provide("anzo.rdf.vocabulary.RDF");


dojo.require("anzo.rdf.Statement");

anzo.rdf.registerNamespace('rdf', "http://www.w3.org/1999/02/22-rdf-syntax-ns#");

anzo.rdf.vocabulary.RDF = {
	
	NAMESPACE : "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
	
	Alt 			: anzo.createURI("rdf:Alt" ),
   	Bag 			: anzo.createURI("rdf:Bag" ),
    Property 		: anzo.createURI("rdf:Property" ),
    Seq 			: anzo.createURI("rdf:Seq" ),
    Statement 		: anzo.createURI("rdf:Statement" ),
    List 			: anzo.createURI("rdf:List" ),
    nil 			: anzo.createURI("rdf:nil" ),

    first 			: anzo.createURI("rdf:first" ),
    rest 			: anzo.createURI("rdf:rest" ),
    subject 		: anzo.createURI("rdf:subject" ),
   	predicate 		: anzo.createURI("rdf:predicate" ),
    object 			: anzo.createURI("rdf:object" ),
    type 			: anzo.createURI("rdf:type" ),
    value 			: anzo.createURI("rdf:value" )	
	
};

