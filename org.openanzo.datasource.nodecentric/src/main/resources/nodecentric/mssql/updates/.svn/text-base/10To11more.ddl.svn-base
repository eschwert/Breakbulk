DROP INDEX STATEMENTS.SRUUID;
DROP INDEX STATEMENTS.SRHS;
DROP INDEX STATEMENTS.SRHP;
DROP INDEX STATEMENTS.SRHO;
DROP INDEX STATEMENTS.SRHC;
DROP INDEX STATEMENTS.SRI;
CREATE INDEX SRUUID ON STATEMENTS (UUID,COMMITTED,RSTART,REND) INCLUDE (NAMEDGRAPHID,SUBJECT,PREDICATE,OBJECT);
CREATE INDEX SRHS ON STATEMENTS (SUBJECT,PREDICATE) WHERE  COMMITTED=0 AND REND IS NULL ;
CREATE INDEX SRHP ON STATEMENTS (PREDICATE,OBJECT) WHERE  COMMITTED=0 AND REND IS NULL;
CREATE INDEX SRHO ON STATEMENTS (OBJECT)  WHERE  COMMITTED=0 AND REND IS NULL;
CREATE INDEX SRHC ON STATEMENTS (COMMITTED DESC,ID) ;
CREATE INDEX SIRC ON STATEMENTS (ID) WHERE COMMITTED=0 AND REND IS NULL;
DROP INDEX STATEMENTS.SMETA_NR;
