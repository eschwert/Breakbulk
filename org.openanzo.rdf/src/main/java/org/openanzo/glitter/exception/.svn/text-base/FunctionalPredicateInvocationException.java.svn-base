package org.openanzo.glitter.exception;

import org.openanzo.exceptions.ExceptionConstants;
import org.openanzo.glitter.query.FunctionalPredicate;

/**
 * Thrown by a {@link FunctionalPredicate} if it encounters an unexpected situation.
 * @author lee <lee@cambridgesemantics.com>
 *
 */
public class FunctionalPredicateInvocationException extends GlitterException {
	private static final long serialVersionUID = 7042208637937406851L;

	/**
	 * 
	 * @param msg More information on the exception.
	 */
	public FunctionalPredicateInvocationException(String msg) {
		super(ExceptionConstants.GLITTER.FUNCTIONAL_PREDICATE_INVOCATION,msg);
	}
	
	/**
	 * Default constructor.
	 */
	public FunctionalPredicateInvocationException() {
		super(ExceptionConstants.GLITTER.FUNCTIONAL_PREDICATE_INVOCATION);
	}
	
}
