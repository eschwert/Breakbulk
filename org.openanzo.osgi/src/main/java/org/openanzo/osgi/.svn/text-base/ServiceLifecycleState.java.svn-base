package org.openanzo.osgi;

/**
 * Enum for the state of the service container
 * 
 * @author Matthew Roy ( <a href="mailto:mroy@us.ibm.com">mroy@us.ibm.com </a>)
 * 
 */
public enum ServiceLifecycleState {
    /** Service has been create */
    CREATED,
    /** Service is stopping */
    STOPPING,
    /** Service has been stopped */
    STOPPED,
    /** Service is starting */
    STARTING,
    /** Service has been started */
    STARTED,
    /** Service not enabled */
    NOT_ENABLED,
}
